{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfeygm"
		},
		"azDbricksLinked_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'azDbricksLinked'"
		},
		"destADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'destADLS'"
		},
		"presentationADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'presentationADLS'"
		},
		"prodADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'prodADLS'"
		},
		"srcJsonADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'srcJsonADLS'"
		},
		"srcSQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'srcSQLDB'"
		},
		"azDbricksLinked_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0607-044521-ihknvvyp"
		},
		"destADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://databrickseystorage.dfs.core.windows.net/"
		},
		"presentationADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://databrickseystorage.dfs.core.windows.net/"
		},
		"prodADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://databrickseystorage.dfs.core.windows.net/"
		},
		"srcJsonADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystoragegm.dfs.core.windows.net/"
		},
		"srcRest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/pragyancloudtrainer/pragyanrepository/main/formula1ds/constructor_results.csv"
		},
		"srcSQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "demodbservergm.database.windows.net"
		},
		"srcSQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksLT"
		},
		"srcSQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"automation_properties_dbrickspipeline_parameters_notebookname": {
			"type": "string",
			"defaultValue": "basic_transform"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfeygm_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azDbricksLinked_accessToken": {
						"value": "[parameters('azDbricksLinked_accessToken')]"
					},
					"destADLS_accountKey": {
						"value": "[parameters('destADLS_accountKey')]"
					},
					"presentationADLS_accountKey": {
						"value": "[parameters('presentationADLS_accountKey')]"
					},
					"prodADLS_accountKey": {
						"value": "[parameters('prodADLS_accountKey')]"
					},
					"srcJsonADLS_accountKey": {
						"value": "[parameters('srcJsonADLS_accountKey')]"
					},
					"srcSQLDB_password": {
						"value": "[parameters('srcSQLDB_password')]"
					},
					"azDbricksLinked_properties_typeProperties_existingClusterId": {
						"value": "[parameters('azDbricksLinked_properties_typeProperties_existingClusterId')]"
					},
					"destADLS_properties_typeProperties_url": {
						"value": "[parameters('destADLS_properties_typeProperties_url')]"
					},
					"presentationADLS_properties_typeProperties_url": {
						"value": "[parameters('presentationADLS_properties_typeProperties_url')]"
					},
					"prodADLS_properties_typeProperties_url": {
						"value": "[parameters('prodADLS_properties_typeProperties_url')]"
					},
					"srcJsonADLS_properties_typeProperties_url": {
						"value": "[parameters('srcJsonADLS_properties_typeProperties_url')]"
					},
					"srcRest_properties_typeProperties_url": {
						"value": "[parameters('srcRest_properties_typeProperties_url')]"
					},
					"srcSQLDB_properties_typeProperties_server": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_server')]"
					},
					"srcSQLDB_properties_typeProperties_database": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_database')]"
					},
					"srcSQLDB_properties_typeProperties_userName": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfeygm_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"automation_properties_dbrickspipeline_parameters_notebookname": {
						"value": "[parameters('automation_properties_dbrickspipeline_parameters_notebookname')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfeygm_ArmTemplate_0')]"
			]
		}
	]
}
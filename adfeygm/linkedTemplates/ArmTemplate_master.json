{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfeygm"
		},
		"destADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'destADLS'"
		},
		"srcJsonADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'srcJsonADLS'"
		},
		"srcSQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'srcSQLDB'"
		},
		"destADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://databrickseystorage.dfs.core.windows.net/"
		},
		"srcJsonADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystoragegm.dfs.core.windows.net/"
		},
		"srcSQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "demodbservergm.database.windows.net"
		},
		"srcSQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksLT"
		},
		"srcSQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfeygm_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"destADLS_accountKey": {
						"value": "[parameters('destADLS_accountKey')]"
					},
					"srcJsonADLS_accountKey": {
						"value": "[parameters('srcJsonADLS_accountKey')]"
					},
					"srcSQLDB_password": {
						"value": "[parameters('srcSQLDB_password')]"
					},
					"destADLS_properties_typeProperties_url": {
						"value": "[parameters('destADLS_properties_typeProperties_url')]"
					},
					"srcJsonADLS_properties_typeProperties_url": {
						"value": "[parameters('srcJsonADLS_properties_typeProperties_url')]"
					},
					"srcSQLDB_properties_typeProperties_server": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_server')]"
					},
					"srcSQLDB_properties_typeProperties_database": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_database')]"
					},
					"srcSQLDB_properties_typeProperties_userName": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}